{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "module": "commonjs",
    "outDir": "lib",
    "lib": ["es5"],
    "target": "es5",
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "paths": {
      "action-helper": [".."]
    },
    "strictNullChecks": true
  },
  "files": ["test.ts"],
  "exclude": ["node_modules"]
}
